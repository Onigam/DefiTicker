{
    "name": "Defi ticker",
    "version": "1.0",
    "description": "A defi available reward extension!",
    "permissions": ["storage", "declarativeContent", "alarms"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "images/defi.png",
        "badge" : "0"
    },
    "manifest_version": 2
}